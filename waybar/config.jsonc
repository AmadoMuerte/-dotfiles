{
    "layer": "top",
    "position": "top",
    "mod": "dock",
    "exclusive": true,
    "passthrough": false,
    "gtk-layer-shell": true,
    "height": 48,
    "modules-left": [
        "clock#Moscow",
        "custom/wakatime"
    ],
    "modules-center": [
        "hyprland/workspaces"
    ],
    "modules-right": [
        "memory",
        "cpu",
        "custom/temperature",
        "disk",
        "pulseaudio",
        "tray",
        "hyprland/language",
        "custom/notification"
    ],
